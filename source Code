#define GL_SILENCE_DEPRECATION
#include <GLUT/glut.h>   // #include<windows.h>
                         //#include <GL/glut.h>   //these 2 library for windows system
#include <stdio.h>
#include<math.h>

// Initialization function
void init(void) {
    glClearColor(1.0, 1.0, 1.0, 0.0);    // Set display window color to white
    glMatrixMode(GL_PROJECTION);         // Set projection parameters
    gluOrtho2D(0.0, 1000.0,0.0, 900.0); // Define the orthographic projection
}

float shift = 0; // a variable used to move the boat right and left
float bx = 10;
int nightMode = 0;
int moon=0;

/* function to make the user able to deal with the system */
void key(int key, int x, int y) {
    switch (key)
    {
    case GLUT_KEY_LEFT:
        shift -= 10;
        glutPostRedisplay();
        break;
    case GLUT_KEY_RIGHT:
        shift += 10;
        glutPostRedisplay();
        break;
    case 'n':
        nightMode = !nightMode;
        glutPostRedisplay();
        break;
    case 'm':
            moon = !moon;
            glutPostRedisplay();
            break;
            
    }
}



void circle(GLfloat rx, GLfloat ry, GLfloat cx, GLfloat cy)
{
    glBegin(GL_POLYGON);
    glVertex2f(cx, cy);
    for (int i = 0; i <= 360; i++)
    {
        float angle = i * 3.1416 / 180;
        float x = rx * cos(angle);
        float y = ry * sin(angle);
        glVertex2f((x + cx), (y + cy));
    }
    glEnd();
}
void tree(GLfloat rx, GLfloat ry, GLfloat cx, GLfloat cy)
{
    glBegin(GL_POLYGON);
    glVertex2f(cx, cy);
    for (int i = 225; i <= 315; i++)
    {
        float angle = i * 3.1416 / 180;
        float x = rx * cos(angle);
        float y = ry * sin(angle);
        glVertex2f((x + cx), (y + cy));
    }
    glEnd();
}


void clouds()
{
    glPushMatrix();
    glTranslatef(bx, 0, 0);
glColor3f(0.996, 0.973, 0.9022); // 1st cloud
glBegin(GL_QUADS);
  glVertex2i(70,700+50);
  glVertex2i(200,700+50);
  glVertex2i(200,720+50);
  glVertex2i(70,720+50);
glEnd();
glColor3f(0.996, 0.973, 0.9022);
 circle(20, 20, 70, 770);
glColor3f(0.996, 0.973, 0.9022);
 circle(25, 25, 70+25, 770+15);
glColor3f(0.996, 0.973, 0.9022);
 circle(35, 35, 70+25+40, 770+15+12);
glColor3f(0.996, 0.973, 0.9022);
 circle(25, 25, 70+25+40+40, 770+15+12-15);
glColor3f(0.996, 0.973, 0.9022);
 circle(15, 15, 70+25+40+40+26, 770+15+12-15-16);
glEnd();
glColor3f(0.996, 0.973, 0.9022); // 2nd cloud
glBegin(GL_QUADS);
  glVertex2i(780+50,680+50);
  glVertex2i(880+50,680+50);
  glVertex2i(880+50,700+50);
  glVertex2i(780+50,700+50);
glEnd();
glColor3f(0.996, 0.973, 0.9022);
 circle(15, 15, 780+50, 745);
glColor3f(0.996, 0.973, 0.9022);
 circle(20, 20, 780+22+50, 745+15);
glColor3f(0.996, 0.973, 0.9022);
 circle(25, 25, 780+22+22+50, 745+15+14);
glColor3f(0.996, 0.973, 0.9022);
 circle(21, 21, 780+22+22+27+50, 745+15+14-16);
glColor3f(0.996, 0.973, 0.9022);
 circle(15, 15, 780+22+22+24+26+50, 745+15+14-15-14);
glEnd();

glColor3f(0.996, 0.973, 0.9022); // 1nst small cloud
glBegin(GL_QUADS);
  glVertex2i(370,670+50);
  glVertex2i(420,670+50);
  glVertex2i(420,685+50);
  glVertex2i(370,685+50);
glEnd();
glColor3f(0.996, 0.973, 0.9022);
 circle(7.5, 7.5, 370.5, 727.5);
glColor3f(0.996, 0.973, 0.9022);
 circle(7.5, 7.5, 370.5+50, 727.5);
glColor3f(0.996, 0.973, 0.9022);
 circle(18, 18, 395, 739);
glColor3f(0.996, 0.973, 0.9022);
glBegin(GL_QUADS);
  glVertex2i(370+260,670+50-20);
  glVertex2i(420+260,670+50-20);
  glVertex2i(420+260,685+50-20);
  glVertex2i(370+260,685+50-20);
glEnd();
glColor3f(0.996, 0.973, 0.9022);
 circle(7.5, 7.5, 370.5+260, 727.5-20);
glColor3f(0.996, 0.973, 0.9022);
 circle(7.5, 7.5, 370.5+50+260, 727.5-20);
glColor3f(0.996, 0.973, 0.9022);
 circle(18, 18, 395+260, 739-20);
glEnd();
    glPopMatrix();
    bx += .2;
    if (bx > 1000)
        bx = -1000;
    glutPostRedisplay();

}

float cx=10;
void car()
{
    glPushMatrix();
    glTranslatef(cx, 0, 0);
glColor3f(0, 0.282, 0.831);  // CAR
glBegin(GL_POLYGON);
  glVertex2i(63,260+10);
  glVertex2i(155,260+10);
  glVertex2i(145,280+10);
  glVertex2i(130,280+10);
  glVertex2i(120,290+10);
  glVertex2i(90,290+10);
  glVertex2i(80,280+10);
  glVertex2i(63,280+10);
glEnd();

glColor3f(0.737, 0.89, 0.894); // car
glBegin(GL_QUADS);
  glVertex2i(82,280+10);
  glVertex2i(130-2,280+10);
  glVertex2i(120-2,290+10-2);
  glVertex2i(90+2,290+10-2);
glEnd();

glLineWidth(2.0f);
glColor3f(0, 0, 0);  // car
glBegin(GL_LINES);
  glVertex2i(105,270);
  glVertex2i(105,300);
glEnd();
glLineWidth(1.0f);
glColor3f(0, 0, 0);  // car
glBegin(GL_LINES);
  glVertex2i(80,290);
  glVertex2i(130,290);
glEnd();

glColor3f(0.278, 0.278, 0.267);
 circle(10, 10, 80, 270);

glColor3f(0.878, 0.875, 0.878);
 circle(5, 5, 80, 270);

glColor3f(0.278, 0.278, 0.267);  //car wheel
 circle(10, 10, 80+50, 270);

glColor3f(0.878, 0.875, 0.878);
 circle(5, 5, 80+50, 270);
glEnd();
    glPopMatrix();
    cx += 1;
    if (cx > 1000)
        cx = -150;
    glutPostRedisplay();

}
float ax=10 ;
void car2()
{
    glPushMatrix();
    glTranslatef(-ax, 0, 0);
glColor3f(0.961, 0.945, 0.91);  // ambulence
glBegin(GL_POLYGON);
  glVertex2i(640,200);
  glVertex2i(740,200);
  glVertex2i(740,240);
  glVertex2i(665,240);
  glVertex2i(655,225);
  glVertex2i(640,220);
glEnd();
glColor3f(0, 0.749, 0.82); //ambulence
glBegin(GL_POLYGON);
glVertex2i(655,225);
  glVertex2i(680,225);
  glVertex2i(680,240-2);
glVertex2i(665-1,240-2);
glEnd();
  glLineWidth(4.0f);
  glColor3f(0.929, 0.031, 0.18);  // ambulance
  glBegin(GL_LINES);
    glVertex2i(640,215);
    glVertex2i(740,215);
glEnd();
glLineWidth(6.0f);
glColor3f(0.929, 0.031, 0.18);  //ambulance
glBegin(GL_LINES);
  glVertex2i(711+.10,220+8);
  glVertex2i(725+.10,220+8);
 glVertex2i(725-7,240-4);
 glVertex2i(725-7,225-4);
glEnd();
glColor3f(0.278, 0.278, 0.267);
 circle(10, 10, 660, 200);

glColor3f(0.878, 0.875, 0.878);  //ambulance wheel
 circle(5, 5, 660, 200);

glColor3f(0.278, 0.278, 0.267);
 circle(10, 10, 670+45, 200);

glColor3f(0.878, 0.875, 0.878);  //ambulance wheel
 circle(5, 5, 670+45, 200);
glEnd();
    glPopMatrix();
    ax += 1;
    if (ax > 800)
        ax = -400;
    glutPostRedisplay();

}
float dx=10;
void chimni()
{
    glPushMatrix();
    glTranslatef(dx, dx,0);
glColor3f(1, 0.965, 0.86);      // chimni cloud 1
 circle(20, 20,230+5, 600+22);
    glPopMatrix();
    dx += .07;
    if (dx > 10)
        dx = -3;
    glutPostRedisplay();

}

float ex=10;
void chimni1()
{
    glPushMatrix();
    glTranslatef(ex, ex, 0);
    glColor3f(1, 0.965, 0.86);      // chimni cloud 2
     circle(17, 17,230+5+20, 600+22+20);

    glPopMatrix();
    ex += .06;
    if (ex > 11)
        ex = -05;
    glutPostRedisplay();

}

float fx=10;
void chimni2()
{
    glPushMatrix();
    glTranslatef(fx, fx, 0);
    glColor3f(1, 0.965, 0.86);      // chimni cloud 3
     circle(7, 7,230+5+20+21, 600+22+20+21);
    glPopMatrix();
    fx += .09;
    if (fx > 12)
        fx = -5;
    glutPostRedisplay();

}



