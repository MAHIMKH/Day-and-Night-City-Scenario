// Main function
int main(int argc, char* argv[]) {

    // Initialize GLUT
    glutInit(&argc, argv);
    glutInitDisplayMode(GLUT_SINGLE | GLUT_RGB); // Set display mode

    glutInitWindowPosition(100, 100);    // Set window position
    glutInitWindowSize(1200, 1100);        // Set window size
    glutCreateWindow("project");  // Create display window

    init();  // Execute initialization procedure

    glutDisplayFunc(drawShapes);  // Call drawShapes function for display
    glutSpecialFunc(key);
    glutMainLoop();  // Start the main event loop

    return 0;
}

